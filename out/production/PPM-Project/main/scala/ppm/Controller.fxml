<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.scala.ppm.Controller">
   <children>
      <GridPane onMouseClicked="#onScaleUpButtonClick" prefHeight="401.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="scaleUpButton" mnemonicParsing="false" onMouseClicked="#onScaleUpButtonClick" text="Scale Up" GridPane.columnIndex="1" />
            <Button fx:id="scaleDownButton" mnemonicParsing="false" onMouseClicked="#onScaleDownButtonClick" text="Scale Down" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="sepiaButton" mnemonicParsing="false" onMouseClicked="#onSepiaButtonClick" text="Sepia" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="removeGreenButton" mnemonicParsing="false" onMouseClicked="#onRemoveGreenButtonClick" text="Remove Green" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Button fx:id="upButton" mnemonicParsing="false" onMouseClicked="#onUpButtonClick" text="Up" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="TOP" />
            <Button fx:id="downButton" mnemonicParsing="false" onMouseClicked="#onDownButtonClick" text="Down" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="BOTTOM" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
